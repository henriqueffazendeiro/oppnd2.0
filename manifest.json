{
  "name": "Gmail Read Ticks",
  "description": "1 tick (n√£o rastreado), 2 cinzentos (enviado), 2 azuis (lido) no Gmail.",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {},
  "permissions": ["storage"],
  "host_permissions": [
    "https://mail.google.com/*",
    "https://oppnd.vercel.app/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "Gmail Read Ticks"
  }
}